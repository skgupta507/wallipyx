{
    "version":2,
    "builds":[
        {
            "src":"./api/index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],"functions": {
        "api/server.js": {
          "memory": 3008,
          "maxDuration": 30
        },
        "api/*.js": {
          "memory": 3008,
          "maxDuration": 30
        }
      }
}